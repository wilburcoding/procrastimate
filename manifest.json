{
  "name": "Unproductive App",
  "description": "Build an Extension!",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/*"
      ],
      "js": [
        "jquery.min.js",
        "script.js"
      ]
    }
  ]
}