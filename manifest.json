{
  "name": "Unproductive App",
  "description": "Productivity at its finest.",
  "version": "1.3",
  "manifest_version": 3,
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "tabs"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "chrome_url_overrides": {
    "newtab": "new-tab.html"
  },
  "background": {
    "service_worker": "script.js"
  },
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "48":"picon.png",
    "64":"picon.png",
    "128": "picon.png"
  }
}